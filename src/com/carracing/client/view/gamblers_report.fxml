<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root minHeight="0.0" minWidth="0.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox minHeight="0.0" minWidth="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="50.0" prefWidth="628.0">
               <children>
                  <Label layoutY="6.0" text="Query" />
                  <ComboBox fx:id="query" layoutX="46.0" layoutY="1.0" prefWidth="150.0" />
                  <TextField fx:id="parameter" layoutX="206.0" layoutY="1.0" prefHeight="26.0" prefWidth="115.0" />
                  <Button fx:id="submit" layoutX="330.0" layoutY="1.0" mnemonicParsing="false" onAction="#submitQuery" text="Submit" />
               </children>
            </AnchorPane>
            <TableView fx:id="gamblersTable" prefHeight="248.0" prefWidth="628.0" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="nameColumn" prefWidth="243.0" text="Name" />
                  <TableColumn fx:id="profitColumn" prefWidth="179.0" text="Profit" />
                  <TableColumn fx:id="numberRacesColumn" prefWidth="159.0" text="Number races" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</fx:root>
